{
    "name": "maxime-rainville/silverstripe-rekognition",
    "description": "Integration between AWS Rekognition and SilverStripe.",
    "type": "silverstripe-vendormodule",
    "license": "MIT",
    "authors": [
        {
            "name": "Maxime Rainville",
            "email": "maxime@silverstripe.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "silverstripe/crontask": "^2.1",
        "silverstripe/framework": "^4",
        "silverstripe/assets": "^1",
        "silverstripe/vendor-plugin": "^1.0",
        "aws/aws-sdk-php": "^3.64"
    },
    "require-dev": {
        "phpunit/PHPUnit": "~4.8",
        "squizlabs/php_codesniffer": "^3"
    },
    "autoload": {
        "psr-4": {
            "MaximeRainville\\SilverstripeRekognition\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MaximeRainville\\SilverstripeRekognition\\Tests\\": "tests/php/"
        }
    },
    "scripts": {
        "lint": "phpcs src/ tests/php/",
        "lint-clean": "phpcbf src/ tests/php/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
